import { Animal } from 'animals'

enum Energy {
  LOW,
  MEDIUM,
  HIGH
}

type Dog extends Animal {
  breed: string
  energy?: Energy
}

type Walker {
  name: string
  walking?: [Dog]
}

query breedsForWalker (walkerId: string): [Dog] {
  SELECT breed {
    $walkerId Walker.walking ?o
    ?o Dog.breed ?breed
  }
}
