import { Location } from 'location-tagger'

type Animal {
  name: string
  limbCount: uint16
  populationCount?: uint64
}

type Human {
  name: string
}

tag type AnimalTag {
  location: Location
  lastUpdate?: uint64
}

sparql query fourLimbedAnimals (): [Animal] {
  SELECT * WHERE {
    ?s Animal.limbCount ?o
    FILTER (?o == 4)
  }
}
