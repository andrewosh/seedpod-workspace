import {
  Location as LocationTag,
  Coordinates
} from 'location-tagger'

type Animal {
  name: string
  limbCount: uint32
  populationCount?: uint64
}

type Human {
  name: string
}

type ExtinctAnimal extends Animal {
  extinctionDate: string
}

type AnimalTag {
  location: LocationTag
  lastUpdate?: uint64
}

query fourLimbedAnimals (): [Animal] {
  SELECT * WHERE {
    ?s Animal.limbCount ?o
    FILTER (?o == 4)
  }
}